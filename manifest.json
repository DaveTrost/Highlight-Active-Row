{
  "name": "Highlight Active Row",
  "version": "1.1.0",
  "description": "Makes the currently selected table row more visible.",
  "web_accessible_resources": [
    "content-scripts/rowHighlighter.js"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/contentScript.js"]
    }
  ],
  "icons": {
    "16": "/assets/highlightActiveRowIcon16.png",
    "48": "/assets/highlightActiveRowIcon48.png",
    "128": "/assets/highlightActiveRowIcon128.png"
  },

  "manifest_version": 2
}
